{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@fst/client/data-access": ["libs/client/data-access/src/index.ts"],
      "@fst/client/feature-dashboard": [
        "libs/client/feature-dashboard/src/index.ts"
      ],
      "@fst/client/feature-login": ["libs/client/feature-login/src/index.ts"],
      "@fst/client/feature-register": [
        "libs/client/feature-register/src/index.ts"
      ],
      "@fst/client/state/*": ["libs/client/data-access/src/lib/state/*"],
      "@fst/client/ui-components/*": ["libs/client/ui-components/src/lib/*"],
      "@fst/client/util": ["libs/client/util/src/index.ts"],
      "@fst/server/data-access": ["libs/server/data-access/src/index.ts"],
      "@fst/server/feature-auth": ["libs/server/feature-auth/src/index.ts"],
      "@fst/server/feature-auth-google": [
        "libs/server/feature-auth-google/src/index.ts"
      ],
      "@fst/server/feature-health": ["libs/server/feature-health/src/index.ts"],
      "@fst/server/feature-todo": ["libs/server/feature-todo/src/index.ts"],
      "@fst/server/feature-user": ["libs/server/feature-user/src/index.ts"],
      "@fst/server/util": ["libs/server/util/src/index.ts"],
      "@fst/server/util/testing": ["libs/server/util/src/lib/testing/index.ts"],
      "@fst/shared/domain": ["libs/shared/domain/src/index.ts"],
      "@fst/shared/util-env": ["libs/shared/util-env/src/index.ts"],
      "@fst/shared/util-testing": ["libs/shared/util-testing/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
